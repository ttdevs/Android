<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ttdevs.android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".AndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EndLessActivity"
            android:label="@string/title_activity_end_less" />
        <activity
            android:name=".UCropActivity"
            android:label="@string/title_activity_ucrop" />
        <activity
            android:name=".ConcealActivity"
            android:label="@string/title_activity_conceal" />
        <activity
            android:name=".TabLayoutActivity"
            android:label="@string/title_activity_tablayout" />
        <activity
            android:name=".MarkdownActivity"
            android:label="@string/title_activity_markdown" />
        <activity
            android:name=".ReactiveActivity"
            android:label="@string/title_activity_reactive" />
        <activity
            android:name=".ViewFlipperActivity"
            android:label="@string/title_activity_view_fillper" />
        <activity
            android:name=".JNIActivity"
            android:label="@string/title_activity_jni" />
        <activity
            android:name=".RouterActivity"
            android:label="@string/title_activity_router"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="article"
                    android:pathPattern="/.*"
                    android:scheme="ttdevs" />
            </intent-filter>
        </activity>
        <activity android:name=".IndicatorViewActivity" />
        <activity
            android:name=".HybridViewActivity"
            android:label="@string/title_activity_hybrid_view"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".WebSocketActivity"
            android:label="@string/title_activity_web_socket"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".SweepGradientActivity" />
        <activity android:name=".CustomViewActivity" />
        <activity android:name=".AboutActivity" />
    </application>

</manifest>